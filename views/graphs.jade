extends layout

block content
  script(src='/javascripts/notifications.js')
  script(src='/javascripts/textreply.js')
  script(src='/javascripts/graphfarmer.js')
  script(src="/javascripts/chart.js")

  div( id='wrapper' )
    div( id="sidebar-wrapper")
      ul.sidebar-nav(id="listOfFarmers")
          li.sidebar-brand
            a( id='recent-notifications', href='#' ) Recent Notifications     

    div( id='page-content-wrapper' )
        div.container-fluid
            div.row
                
                h2 Profit
                br
                canvas#canvasProfit(height='200',width='500')
                br
                br
                h2 Number of Honey Jars Sold
                br
                canvas#canvasJars(height='200',width='500')
                br
                br
                h2 Costs
                br
                canvas#canvasCosts(height='200',width='500')
                br
                br
                h2 Number of Hours Worked
                br
                canvas#canvasHours(height='200',width='500')

